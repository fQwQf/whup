j=1;
sum=0;
while(j<=100)
{
    sum = 0;
    i=3;
    
    while(i<=100000)
    {
        isprime=1;
        b=2;
        while(b*b<=i)
        {
            if(i%b==0)
            {
                isprime=0;
            }
            b=b+1;
        }
        if(isprime==1)
        {
            sum=sum+1;
        }
        i=i+1;
    }
    j=j+1;
}
